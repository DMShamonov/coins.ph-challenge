// @flow

export type ProfileType = {|
  name: string,
  username: string,
  email: string,
|};

export type WalletType = {
  id: string,
  label: string,
  balance: number,
};

export type WalletsType = Array<WalletType>;

export type WalletTransactionType = {
  id: string,
  date: string,
  entries: Array<{
    account: string,
    value: number,
  }>,
  fee: number,
  pending: boolean,
};

export type WalletTransactionsType = Array<WalletTransactionType>;
